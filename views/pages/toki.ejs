<!DOCTYPE html> 
<html>
  <head>
    <meta charset="utf-8">
    <title>All Tokimons</title>
    <link rel="stylesheet" href="/Tokimon.css" />
  </head>
  <body>
    <h1>Selected Tokimon</h1>
    <table>
        <thead>
            <td>Name</td>
            <td>Trainer</td>
            <td>Height</td>
        </thead>
        <tbody>
            <% result.forEach((r) => { %>
                <tr>
                    <td><%= r.name ? r.name : '' %></td>
                    <td><%= r.trainer ? r.trainer : '' %></td>
                    <td><%= r.height ? r.height : '' %></td>
                </tr>
                <% }); %>
            </tbody>
        </table>
    <button class="button1" onClick="javascript:window.location.href='/tokimon.html'">Return to Home Page</button>
  </body>
</html>